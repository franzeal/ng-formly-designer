{
  "name": "ngx-formly-designer-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "debug": "concurrently \"ng serve -c development\"  \"ng build ngx-formly-designer --watch\"",
    "start": "npm run build_lib && ng serve --prod",
    "build": "ng build ngx-formly-designer && ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copy": "node tools/copy.js README.md dist/ngx-formly-designer/README.md && node tools/copy.js LICENSE dist/ngx-formly-designer/LICENSE",
    "build_lib": "ng build ngx-formly-designer --prod",
    "npm_pack": "npm run copy && cd dist/ngx-formly-designer && npm pack",
    "package": "npm run build_lib && npm run npm_pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@ngx-formly/bootstrap": "^5.10.21",
    "@ngx-formly/core": "^5.10.21",
    "bootstrap": "^4.3.1",
    "core-js": "^3.2.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lodash-es": "^4.17.15",
    "ngx-formly-tabs": "^5.3.0",
    "popper.js": "^1.15.0",
    "rxjs": "^6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^10.2.5",
    "@angular-devkit/build-ng-packagr": "~0.1002.3",
    "@angular-devkit/build-angular": "~0.1002.3",
    "@angular/cli": "~10.2.3",
    "@angular/language-service": "^10.2.5",
    "@types/jquery": "~3.3.31",
    "@types/lodash-es": "~4.17.3",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ng-packagr": "^10.1.0",
    "typescript": "4.0.8",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0"
  }
}
